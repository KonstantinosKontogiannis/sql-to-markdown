{
    "name": "slince/sql-to-markdown",
    "description": "Convert mysql table schema to markdown table",
    "require": {
        "phpmyadmin/sql-parser": "^5.0",
        "symfony/console": "^4.3",
        "maddhatter/markdown-table": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Slince\\SqlToMarkdown\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slince\\SqlToMarkdown\\Tests\\": "./tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    }
}
